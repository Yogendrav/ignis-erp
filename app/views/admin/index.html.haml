%h1= get_view_text('h1')
.container
  #content
    %ul#tabs.nav.nav-tabs{data: {tabs: 'tabs'}}
      %li.active
        %a{data: {toggle: 'tab'}, href: '#users'}
          %b= t('label.manage_users')
      %li
        %a{data: {toggle: 'tab'}, href: '#logs'}
          %b= t('label.logs')
    #my-tab-content.tab-content
      #users.tab-pane.active
        %br/
        #user_list
          = render 'admin/partials/users'
      #logs.tab-pane
        %br/
        #devise_usage_report_block
          = render 'devise_usage_log/partials/devise_usage_report_link'

%br/
%br/
%br/
